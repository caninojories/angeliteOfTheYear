  .row-centered {
    text-align:center;
  }
  .col-centered {
    display:inline-block;
    float:none;
    /* reset the text-align */
    text-align:left;
    /* inline-block space fix */
    margin-right:0;
  }
  .frame {
    background-color: #e67e22;
    h3, p {
      color: #fff;
    }
  }
  .nonTeaching {
    background-color: #e74c3c;
    h3, p {
      color: #fff;
    }
  }
  .student {
    background-color: #d35400;
    h3, p {
      color: #fff;
    }
  }
  .org {
    background-color: #c0392b;
    h3, p {
      color: #fff;
    }
  }
  .teachingCategory {
    color: #e67e22;
    .label-default {
      background-color: #e67e22;
    }
  }
  .nonTeachingCategory {
    color: #e74c3c;
    .label-default {
      background-color: #e74c3c;
    }
  }
  .studentCategory {
    color: #d35400;
    .label-default {
      background-color: #d35400;
    }
  }
  .orgCategory {
    color: #c0392b;
    .label-default {
      background-color: #c0392b;
    }
  }
